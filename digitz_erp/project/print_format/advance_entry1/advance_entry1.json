 "html": "{% set customer = frappe.get_doc(\"Customer\",doc.customer) %} \n{% set company = frappe.get_doc(\"Company\",doc.company)%}\n\n<div class=\"box\" style=\"height:fit-content;border-left: 1px solid #000 !important; border-right: 1px solid #000 !important;\">\n    <div id=\"header-html\" class=\"hidden-pdf\">\n        {% if letter_head and not no_letterhead %}\n        <div class=\"letter-head\">{{ letter_head }}</div>\n        {% endif %}\n    </div>\n\n    <div class=\"status\" style=\"text-align: center;font-size:20px;height:35px; border-top: 1px solid black; background-color: #fff !important;width: 100%;padding-top:5px;\">\n        <b>Advance Entry</b>\n    </div>\n\n    <div class=\"box2\">\n        <div class=\"main2\" style=\"width: 100%; max-width: 100%; border-top: 1px solid black;width: 100%;\">\n            <div class=\"main_party\" style=\"float:left;width: 55%;min-height:100px;border-right:1px solid black; pedding:5px;\">\n                <div class=\"byte-add\" style=\"float:left;width: 15%; pedding:5px;margin-top:5px\">\n                    <b style=\"margin-left:5px;padding-top:10px\">Bill To:</b>\n                    <br>\n                </div>\n                <div class=\"byte-add\" style=\"float:left;width: 85%; pedding:5px;margin-top:5px\">\n                    <b style=\"margin-left:10px;\">{{customer.customer_name}}</b>\n                    <br>\n                    \n                    <p style=\"margin-left:10px;\">\n                    {% if customer.area %}\n                    {{ customer.area_name or '' }},<br>\n                    {% endif %}\n                    {% if customer.emirate %}\n                    {{ customer.emirate}},<br>\n                    {% endif %}\n                    {% if customer.country %}\n                    {{ customer.country }}\n                    {% endif %}\n                    </p>\n                </div>\n            </div>\n            <div class=\"main_party\" style=\"float:left;width: 40%; pedding:5px;\">\n                <div class=\"byte-add\" style=\"float:left;width: 40%; pedding:5px;\">\n                    <p style=\"margin-left:5px;font-weight:600;margin-bottom:10px;margin-top:10px;\">Entry No:</p>\n                    <p style=\"margin-left:5px;font-weight:600;margin-bottom:10px;margin-top:10px;\">Entry Date:</p>\n                </div>\n                <div class=\"byte-add\" style=\"float:left;width: 60%; pedding:5px;\">\n                    <p style=\"margin-left:10px;font-weight:600;margin-bottom:10px;margin-top:10px;\">{{doc.name}}</p>\n                    <p style=\"margin-left:10px;font-weight:600;margin-bottom:10px;margin-top:10px;\">{{doc.posting_date}}</p>\n                </div>\n            </div>\n        </div>\n    </div>\n\n    <div class=\"items_table\" style=\"margin-top:2px;\">\n        <table class=\"table\" style=\"margin-left:1px; margin-right:1px;\">\n            <thead>\n                <tr>\n                    <th style=\"border-top: 1px solid #000 !important; border-bottom: 1px solid #000 !important; width:20px; text-align:center;\"># </th>\n                    <th style=\"border-top: 1px solid #000 !important; border-bottom: 1px solid #000 !important;border-left:1px solid;width:250px; text-align:center;\"><b>Description</b></th>\n                    <th style=\"border-top: 1px solid #000 !important; border-bottom: 1px solid #000 !important; border-left:1px solid; width:60px; text-align:center;\"><b>Tax Type</b></th>\n                    <th style=\"border-top: 1px solid #000 !important; border-bottom: 1px solid #000 !important; border-left:1px solid; width:60px; text-align:center;\"><b>Tax Rate</b></th>\n                    <th style=\"border-top: 1px solid #000 !important; border-bottom: 1px solid #000 !important; border-left:1px solid; width:100px; text-align:center;\"><b>Tax Amount</b></th>\n                    <th style=\"border-top: 1px solid #000 !important; border-bottom: 1px solid #000 !important;min-width:125px; border-left:1px solid;border-right:1px solid; text-align:center;\"><b>Total Amount</b></th>\n                </tr>\n            </thead>\n            <tbody>\n                {% for row in doc.advance_item_table %}\n                {% set tax_doc = frappe.get_doc(\"Tax\", row.tax) if row.tax else None %}\n                <tr>\n                    <td style=\"border-top: 1px solid #000 !important; border-bottom: 1px solid #000 !important; width: 20px; text-align: center;\">{{ row.idx }}</td>\n                    <td style=\"border-top: 1px solid #000 !important; border-bottom: 1px solid #000 !important; border-left: 1px solid; width: 100px; text-align: center;\">{{ row.description or '' }}</td>\n                    <td style=\"border-top: 1px solid #000 !important; border-bottom: 1px solid #000 !important; border-left: 1px solid #000 !important; width: 110px; text-align: center;\">{{ tax_doc.tax_name if tax_doc else 'None' }}</td>\n                    <td style=\"border-top: 1px solid #000 !important; border-bottom: 1px solid #000 !important; border-left:1px solid #000 !important; width:87px; text-align:center;\">{{row.tax_rate or '0'}}</td>\n                    <td style=\"border-top: 1px solid #000 !important; border-bottom: 1px solid #000 !important; border-left:1px solid #000 !important; width:100px; text-align:center;\">{{row.tax_amount or '0'}}</td>\n                    <td style=\"border-top: 1px solid #000 !important; border-bottom: 1px solid #000 !important; border-left:1px solid; border-right:1px solid; text-align:center; width:100px;\">{{ row.net_amount }}</td>\n                </tr>\n                {% endfor %}\n            </tbody>\n        </table>\n    </div>\n\n   <div class=\"main6\">\n    <table class=\"table1\" style=\"width: 45.3%; border-left: 1px solid; float: right; border-bottom: none !important; margin-top: -21px; border-top: none !important; clear: left;border-right:3px solid black\">\n        <thead>\n            <tr style=\"border-bottom: 1px solid !important;\">\n                <td style=\"border-right: 1px solid; width: 42.5%;\">Total Amount</td>\n                {% set net_total = namespace(value=0.0) %}\n                {% for amt in doc.advance_item_table %}\n                    {% set net_total.value = net_total.value + (amt.net_amount | float) %}\n                {% endfor %}\n                <td style=\"text-align: right;\"><b>{{ \"%.2f\"|format(net_total.value) }}</b></td>\n            </tr>\n        </thead>\n    </table>\n</div>\n\n\n\n    <!--<div class=\"main-b\" style=\"border-left: 1px solid black;border-right: 1px solid black; width:45.1%;clear:both !important;text-align:left; float:right;margin-bottom: 5px;border-top: 1px solid black;border-bottom: 1px solid black;\">-->\n    <!--    <p style=\"font-size: x-small;margin-left: 5px;text-align:left;\"><b>Total In Words : </b>{{doc.in_words}}</p>-->\n    <!--</div>-->\n    \n    \n    {%if doc.terms%}\n    <div style=\"margin-top:5px;width:100%\">\n    <br>\n    <br>\n    {%if doc.terms !='<div class=\"ql-editor read-mode\"><p><br></p></div>'%}\n     <div class=\"status\" style=\"margin-top:5px;font-size:15px;height:30px; background-color: #fff !important;width: 100%;border-top:1px solid black;border-bottom:1px solid black\">\n        <b style=\"font-size:1.2rem;padding-left:30%\">Terms and Conditions Details</b>\n    </div>\n    <div>\n        <p style=\"margin-left:10px;margin-top:12px\">\n           {{doc.terms}}\n        </p>\n    </div>\n        {%endif%}\n        {%endif%}\n    </div>\n\n    <div class=\"main10\" style=\"margin-top:150px; height:160px; width: 100%; max-width: 100%;page-break-inside: avoid; break-inside: avoid;-webkit-column-break-inside: avoid;border: 1px solid black\">\n        <div class=\"main_party10\" style=\"float:left;width: 55%;margin-left:-1px;margin-right:2px;height:100%; pedding:5px;margin-top:0px\">\n            <div class=\"byte-add10\" style=\"float:left;width: 35%;font-size:12px; pedding:5px; \">\n                <div style=\"margin-bottom:7px;margin-top:5px\"><b style=\"margin-left:5px;\">Bank Details:</b></div>\n                <div style=\"margin-bottom:10px;\">\n                    <b style=\"margin-left:5px; padding-bottom:15px !important;\">Company Name:</b>\n                </div>\n                <!--<div style=\"margin-bottom:10px;\">-->\n                <!--    <b style=\"margin-left:5px; margin-bottom:15px;\">RTGS / NEFT IFSC:</b>-->\n                <!--</div>-->\n                <div style=\"margin-bottom:10px;\">\n                    <b style=\"margin-left:5px; margin-bottom:15px;\">Bank Account No.:</b>\n                </div>\n                <!--<div style=\"margin-bottom:10px;\">-->\n                <!--    <b style=\"margin-left:5px;\">Company's PAN:</b>-->\n                <!--</div>-->\n            </div>\n            <div class=\"byte-add10\" style=\"float:left;font-size:12px; pedding:5px; width:65%;margin-top:2px\">\n                <div style=\"margin-bottom:10px;\">\n                    <b></b>\n                    <br>\n                </div>  \n                <div style=\"margin-bottom:10px;\">\n                    <p style=\"margin-left:5px;font-weight:400\"><b>{{company.name}}</b></p>\n                </div>\n                <!--<div style=\"margin-bottom:10px;\">-->\n                <!--    <p style=\"margin-left:5px;font-weight:400\"><b>{{company.pan}}</b></p>-->\n                <!--</div>-->\n            </div>\n        </div>\n        <div class=\"main_party10\" style=\"float:left;width: 45%; font-size:12px; height:100%; pedding:5px; text-align:center; border-left: 1px solid #000 !important; margin-top:0px;\">\n            <div style=\"text-align:center;margin-top:10px; margin-bottom:-5px;\">\n                <b>For {{company.name}}</b>\n            </div>\n            <br>\n            <img src=\"/files/sign.png\" style=\"height:64px; width: 80px;margin:10px;\">\n            <br>\n            <b>Authorised Signatory</b>\n        </div>\n    </div>\n    <div class=\"thanks\" style=\"text-align: center; font-size: 15px; background-color: #fff !important; width: 100%;border:1px solid black\">\n        <b><i>Thank You</i></b>\n    </div>\n</div>",
