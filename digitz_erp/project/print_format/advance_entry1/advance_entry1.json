 "css": ".print-format{\n    padding-top:0mm !important;\n    padding-bottom:0mm !important;\n    padding-left:0mm !important;\n    padding-right:0mm !important;\n    margin-top:60mm !important;\n    margin-left:2mm !important;\n    margin-right:2mm !important;\n}\n\n.letter-head{\n    margin-bottom:-10px;\n}\n/*.custom_width{*/\n/*    width:70px;*/\n/*}*/\n\n/*.items_table .table,*/\n/*    .main6 .table1 {*/\n/*        table-layout: auto;*/\n/*        width: 100%;*/\n/*    }*/\n/*.items_table .table th,*/\n/*    .items_table .table td,*/\n/*    .main6 .table1 td {*/\n/*        white-space: nowrap;*/\n/*        padding: 5px;*/\n/*    }*/",
