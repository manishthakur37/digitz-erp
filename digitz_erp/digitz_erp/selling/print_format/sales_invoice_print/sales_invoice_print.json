 "html": "<!DOCTYPE html>\r\n<html lang=\"en\">\r\n<head>\r\n    <meta charset=\"UTF-8\">\r\n    <meta http-equiv=\"X-UA-Compatible\" content=\"IE=edge\">\r\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\r\n    <link rel=\"stylesheet\" href=\"https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/css/bootstrap.min.css\">\r\n    <script src=\"https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js\"></script>\r\n    <script src=\"https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/js/bootstrap.min.js\"></script>\r\n    <title>Document</title>\r\n    </head>\r\n\r\n   \r\n<body style=\"font-family: Inconsolata Regular!important;font-size: 11px!important;\">\r\n    \r\n    <div class=\"container-fluid mainpf\" style=\"width:100%;\">\r\n        \r\n        <table class=\"table table-bordered\" >\r\n            <tr style=\"width:100%;\">\r\n                <td style=\"width:45%;border-right:1px!important;border-right:1px!important;\"><img style=\"\" src=\"https://silverspace.frappe.cloud/files/Logo-no-bg.png\" ></td>\r\n                <td style=\"width:50%;border-left:0px!important;align:center; text-align: center;\" >\r\n                     <font style=\"font-size:10px;margin-left:10px;\"><br>309 Sharina 3,Diera waterfront AI Daghayah Dubai, <br> Ph: +971 58 577 4199, +971 4 235 3258 Email:info@silverspacecctv.com \r\n                     <br>TRN No:100209579000003\r\n                     </font>\r\n                </td>\r\n                \r\n            </tr>\r\n                \r\n\r\n            <!--</tr>-->\r\n            \r\n            </table>\r\n            \r\n            <table class=\"table table-bordered\" style=\"margin-top:-21px; padding-bottom:0px;\">\r\n            <tr style=\"width:100%;\">\r\n                <td style=\"width:100%;border-bottom:1px!important; align:center; text-align: center; \" colspan=\"3\">\r\n                    <!--<p style=\"font-size:15px;margin-left:10px;\"><b>Silver Space Electromechanical Works LLC </b></p>-->\r\n                     <font style=\"font-size:22px; padding-top:45px;font-family: Inconsolata Regular !important;\"> Tax Invoice </font><br>\r\n                     <font style=\"font-size:18px;font-family: Inconsolata Regular !important;\">{{ doc.name }}</font>\r\n                </td>\r\n                \r\n            </tr>\r\n        </table>\r\n        \r\n        \r\n        <!--<table class=\"table table-bordered t1\" style=\"margin-top:-20px!important; width:100%;\">-->\r\n                \r\n                   \r\n                  <table class=\"table table-bordered\" style=\"margin-top:-20px!important;\" >\r\n                      <tr style=\"width:100%;\">\r\n                          <td style=\"width:100%;background-color:#EEEEEE!important;\"><b>Bill To:</b></td>\r\n                      </tr> \r\n                 </table>\r\n                    \r\n            <table class=\"table table-bordered\" style=\"margin-top:-20px!important;\" >\r\n                  <tr style=\"width:100%;\">\r\n                     <td style=\"width:50%;border-top: 0px!important\" > <b>{{doc.customer}}</b><br>\r\n                     \r\n                     \r\n                     \r\n                                {%if doc.address_display %}\r\n                                {{ doc.address_display}}<br>\r\n                                {% endif %} \r\n                                \r\n                                {%if doc.contact_display %}\r\n                                <b>Contact Person:</b> &nbsp;{{ doc.contact_person }}<br>\r\n                                {% endif %} \r\n                                \r\n                                {%if doc.contact_mobile %}\r\n                                     <b> Mobile No</b> : &nbsp; {{ doc.contact_mobile }}<br>\r\n                                {% endif %} \r\n                                {%if doc.contact_email %}\r\n                                      <b>Contact Email</b> : &nbsp;  {{ doc.contact_email }}<br>\r\n                                    \r\n                                {% endif %}  \r\n                                {%if doc.tax_id %}\r\n                                      <b>TRN No.</b> : &nbsp;  {{ doc.tax_id }}\r\n                                {% endif %}\r\n                                \r\n                                {%if doc.customer == \"Sadeem Building Material Co. LLC\" %} <br>\r\n                                      <b>TRN No.</b> : &nbsp;  <b>100000517100003</b>\r\n                                {% endif %} \r\n                        \r\n                        \r\n                                \r\n                      </td>\r\n                      \r\n                      <td style=\"text-align:left; border:2px; width:50%;\" colspan:2;>\r\n                          <!--<b>Qtn No</b> : &nbsp;  {{ doc.name }} <br/>-->\r\n                          \r\n                          <b>Invoice Date</b> : &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;\r\n                          {{frappe.utils.get_datetime(doc.posting_date).strftime(\"%d-%m-%Y\")}}<br>\r\n                          <b>Due Date</b> : &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;\r\n                          {{ frappe.utils.get_datetime(doc.due_date).strftime(\"%d-%m-%Y\") }} \r\n                          \r\n                      </td>\r\n                      \r\n                      \r\n                  </tr>\r\n                 \r\n            </table>\r\n            \r\n            \r\n                      <table class=\"table table-bordered\" style=\"margin-top:-20px!important;\">\r\n                          <thead style=\"\">\r\n                        <tr>\r\n                            <th style=\"width:15px;font-size:11px;background-color:#EEEEEE!important;\"># </th>\r\n                            <th style=\"width:150px;font-size:11px;background-color:#EEEEEE!important;\">Item & Description</th>\r\n                            <th style=\"width:15px;font-size:11px;background-color:#EEEEEE!important;\">Qty </th>\r\n                             <!--<th style=\"width:15px;font-size:11px;background-color:#EEEEEE!important;\">Discount Amount</th>-->\r\n                            <th style=\"width:15px;font-size:11px;background-color:#EEEEEE!important;\">Rate</th>\r\n                            <th style=\"width:15px;font-size:11px;background-color:#EEEEEE!important;\">Tax %</th>\r\n                            <th style=\"width:15px;background-color:#EEEEEE!important;\">Tax</th>\r\n                            <th style=\"width:15px;font-size:11px;background-color:#EEEEEE!important;\">Amount</th>\r\n    \r\n                        </tr>\r\n                    </thead>\r\n\r\n                    <tbody>\r\n                        {% for i in doc.items %}\r\n                        <tr>\r\n                            <td style=\"font-size:11px;\">{{i.idx}}</td>\r\n                            <td style=\"font-size:11px;\">{{i.item_code}} - {{i.item_name}}</td>\r\n                            <td style=\"font-size:11px;\">{{i.qty}} pcs</td>\r\n                            <!--<td style=\"font-size:11px;\">{{i.discount_amount}}</td>-->\r\n                            <td style=\"font-size:11px;text-align:center;\">{{i.rate}}</td>\r\n                            <td style=\"font-size:11px;text-align:center;\">{{i.tax_rate}}</td>\r\n                            <td style=\"font-size:11px; text-align:center;\">{{i.tax_amount}}</td>\r\n                            <td style=\"font-size:11px;text-align:center;\">{{i.amount}}</td>   \r\n                        </tr>\r\n                        \r\n                        {% endfor %}\r\n                       \r\n                    </tbody>\r\n                    </table>\r\n                 <table class=\"table table-bordered\" style=\"margin-top:-20px!important;\"> \r\n                        <tr>\r\n                            <td colspan=\"4\" style=\"width:40%;\">  \r\n                                <p style=\"margin-top:30px!important;\">\r\n                                Total In Words<br>\r\n                                <b>{{doc.in_words}}</b>\r\n                                </p>\r\n                            </td>\r\n                            <td colspan=\"8\" style=\"width:60%;\">\r\n                                <!--<div>-->\r\n                                    <table style=\"width:100%;font-size:11px;text-align: end;font-family: Inconsolata Regular !important;border:0px !important;\">\r\n                      \r\n                                          <tr>\r\n                                            <td style=\"width:120px;text-align: end;border-right:0px!important;border-left:0px!important;border-bottom:0px!important;border-top:0px!important;\">Sub Total</td>\r\n                                            <td style=\"width:100px;margin-right:15px!important;border-right:0px!important;border-left:0px!important;border-bottom:0px!important;border-top:0px!important;\">{{doc.total}}</td>\r\n                                          </tr>\r\n                                          \r\n                                           <tr>\r\n                                            <td style=\"width:120px;border-right:0px!important;border-left:0px!important;border-bottom:0px!important;border-top:0px!important;text-align: end;\">Additional Discount Amount</td>\r\n                                            <td style=\"width:100px;margin-right:15px!important;border-right:0px!important;border-left:0px!important;border-bottom:0px!important;border-top:0px!important;text-align:right;\">{{doc.discount_amount}}</td>\r\n                                          </tr>\r\n                                          \r\n                                          \r\n                                          <tr>\r\n                                            <td style=\"width:120px;text-align: end;border-right:0px!important;border-left:0px!important;border-bottom:0px!important;border-top:0px!important;\">Standard Rate (5%)</td>\r\n                                            <td style=\"width:100px;text-align: right;border-right:0px!important;border-left:0px!important;border-bottom:0px!important;border-top:0px!important;\">{{doc.tax_amount}}</td>\r\n                                          </tr>\r\n                                          <tr>\r\n                                            <td style=\"width:120px;text-align: end;border-right:0px!important;border-left:0px!important;border-bottom:0px!important;border-top:0px!important;\">Zero Rate (0%)</td>\r\n                                            <td style=\"width:100px;text-align: end;border-right:0px!important;border-left:0px!important;border-bottom:0px!important;border-top:0px!important;\"></td>\r\n                                          </tr>\r\n                                          \r\n                                          <tr>\r\n                                            <td style=\"width:120px;text-align: end;border-right:0px!important;border-left:0px!important;border-bottom:0px!important;border-top:0px!important;\"><b>Total</b></td>\r\n                                            <td style=\"width:100px;text-align:right;border-right:0px!important;border-left:0px!important;border-bottom:0px!important;border-top:0px!important;\"><b>AED {{doc.grand_total}}</b></td>\r\n                                          </tr>\r\n                                          \r\n                                          <tr>\r\n                                            <td style=\"width:120px;text-align: end;border-right:0px!important;border-left:0px!important;border-bottom:0px!important;border-top:0px!important;\"><b>Paid Amount</b></td>\r\n                                            <td style=\"width:100px;text-align:right;border-right:0px!important;border-left:0px!important;border-bottom:0px!important;border-top:0px!important;color:red;\"><b>(-) {{doc.grand_total-doc.outstanding_amount}}</b></td>\r\n                                          </tr>\r\n                                          <tr>\r\n                                            <td style=\"width:120px;text-align: end;border-right:0px!important;border-left:0px!important;border-bottom:0px!important;border-top:0px!important;\"><b>Balance</b></td>\r\n                                            <td style=\"width:100px;text-align:right;border-right:0px!important;border-left:0px!important;border-bottom:0px!important;border-top:0px!important;\"><b>AED {{doc.outstanding_amount}}</b></td>\r\n                                          </tr>\r\n\r\n\r\n                                    </table>\r\n                                <!--</div>-->\r\n                               \r\n                                \r\n                            </td>\r\n                        </tr>\r\n                        </table>\r\n                        \r\n                       \r\n                        <p>\r\n                            <h4 style=\"margin-left:10px;border-top: 0px!important;font-size: 20px;\">Tax Summary</h4>\r\n                        \r\n                        \r\n                        <!--<td style=\"border-right: 0px!important;\"><h4 style=\"margin-left:10px;border-top: 0px!important;font-size: 20px;\">Tax Summary</h4></td>-->\r\n                        \r\n                        <table class=\"table table-bordered\" style=\"width:100%;\">\r\n                          <tr>\r\n                            <td style=\"background-color:lightgray;border-left:0px!important;\">Tax Details</td>\r\n                            <td style=\"background-color:lightgray;text-align:end;\">Taxable Amount (AED)</td>\r\n                            <td style=\"background-color:lightgray;text-align:end;\">Tax Amount (AED)</td>\r\n                          </tr>\r\n                          <tr>\r\n                            <td style=\"border-left:0px!important;\">Zero Rate (0%)</td>\r\n                            <td style=\"text-align:end;border-right: 0px!important;\"></td>\r\n                            <td style=\"text-align:end;border-right:0px!important;\"></td>\r\n                          </tr>\r\n                          \r\n                           <tr>\r\n                            <td style=\"\">Standard Rate (5%)</td>\r\n                            <td style=\"text-align:end;\">{{doc.total}}</td>\r\n                            <td style=\"text-align:end;border-right:0px!important;border-bottom: 0px!important;border-left: 0.5px solid black!important;\">{{doc.tax_amount}}</td>\r\n                          </tr>\r\n                         \r\n                          \r\n                           <tr>\r\n                            <td style=\"border-left:0px!important;border-top:0px solid black!important;\"><b>Total</b></td>\r\n                            <td style=\"text-align:end;border-top:0.5px solid black!important;border-right: 0px!important;\"><b>{{doc.total}} AED </b></td>\r\n                            <td style=\"text-align:end;\"><b>{{doc.base_rounded_total}} AED </b></td>\r\n                          </tr>\r\n                        </table>\r\n        \r\n                        \r\n                        <div>\r\n                            <p style=\"font-size:11px;margin-left:10px;font-family: Calibri!important;\">\r\n                                Thanks for your business.<br><br>\r\n                                <!--Terms & Conditions<br>-->\r\n                                Cash Payment / Cheque in our company name or Bank Transfer to<br>\r\n                                M/S SILVERSPACE ELECTROMECHANICAL WORKS LLC<br>\r\n                                Bank of Baroda Deira - Branch<br>\r\n                                Account No: 900 302 000 068 37<br>\r\n                                IBAN:AE 140 110 090 030 200 006 837<br>\r\n            